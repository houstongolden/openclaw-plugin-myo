#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/openclaw@2026.2.1_@napi-rs+canvas@0.1.89_@types+express@5.0.6_node-llama-cpp@3.15.1_typ_727b8e688408741144073d4b92d0ed6c/node_modules/openclaw/node_modules:/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/openclaw@2026.2.1_@napi-rs+canvas@0.1.89_@types+express@5.0.6_node-llama-cpp@3.15.1_typ_727b8e688408741144073d4b92d0ed6c/node_modules:/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/openclaw@2026.2.1_@napi-rs+canvas@0.1.89_@types+express@5.0.6_node-llama-cpp@3.15.1_typ_727b8e688408741144073d4b92d0ed6c/node_modules/openclaw/node_modules:/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/openclaw@2026.2.1_@napi-rs+canvas@0.1.89_@types+express@5.0.6_node-llama-cpp@3.15.1_typ_727b8e688408741144073d4b92d0ed6c/node_modules:/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../openclaw.mjs" "$@"
else
  exec node  "$basedir/../../openclaw.mjs" "$@"
fi
