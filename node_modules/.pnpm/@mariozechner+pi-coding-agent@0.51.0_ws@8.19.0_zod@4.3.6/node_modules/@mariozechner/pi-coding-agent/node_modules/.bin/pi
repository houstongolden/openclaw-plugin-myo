#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/@mariozechner+pi-coding-agent@0.51.0_ws@8.19.0_zod@4.3.6/node_modules/@mariozechner/pi-coding-agent/dist/node_modules:/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/@mariozechner+pi-coding-agent@0.51.0_ws@8.19.0_zod@4.3.6/node_modules/@mariozechner/pi-coding-agent/node_modules:/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/@mariozechner+pi-coding-agent@0.51.0_ws@8.19.0_zod@4.3.6/node_modules/@mariozechner/node_modules:/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/@mariozechner+pi-coding-agent@0.51.0_ws@8.19.0_zod@4.3.6/node_modules:/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/@mariozechner+pi-coding-agent@0.51.0_ws@8.19.0_zod@4.3.6/node_modules/@mariozechner/pi-coding-agent/dist/node_modules:/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/@mariozechner+pi-coding-agent@0.51.0_ws@8.19.0_zod@4.3.6/node_modules/@mariozechner/pi-coding-agent/node_modules:/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/@mariozechner+pi-coding-agent@0.51.0_ws@8.19.0_zod@4.3.6/node_modules/@mariozechner/node_modules:/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/@mariozechner+pi-coding-agent@0.51.0_ws@8.19.0_zod@4.3.6/node_modules:/Users/houstongolden/Desktop/CODE_2026/openclaw-plugin-myo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
